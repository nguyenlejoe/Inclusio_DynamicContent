ember\",\n        pos: \"Position\",\n        dep: \"Department\",\n        spec: \"Specialization\",\n        status: \"Overdue\" ,\n    }\n]\n\nconst Profile = ({members, onDelete}) => {\n    const [current, setCurrent] = useState(null);\n    const [z, setZ] = useState(null);\n\n    const HandleClicked = (id)=>{\n        if(current === id){\n            setZ(-2)\n            setTimeout(function(){ setCurrent(null); }, 100);\n        }else{\n            console.log(\"open\");\n            setZ(-2)\n            setTimeout(function(){ setCurrent(id); }, 100);\n            setTimeout(function(){ setZ(1); }, 500);\n\n        }\n    }\n    return (\n        <CompContainer>\n            {members && members.map(o=> <Container left={current === o.id ? '-200px' : '0px'}>\n                <ProfileCont>\n                    <Content>\n                        <Avatar bgimg={o.img} onClick={onDelete}/>\n                        <Info>\n                            <h3>{o.name}</h3>\n                            <span><p>{o.name}</p></span>\n                            <span><p>{o.pos}</p></span>\n                            <span><p>{o.dep}</p></span>\n                            <p>{o.spec}</p>\n                            <Status statusText={o.status}/>\n                        </Info>\n                        <Expand onClick={()=>{HandleClicked(o.id)}}>\n                        <img src={\"expand.svg\"}/>\n                        </Expand>\n                    </Content>\n                    <Divider/>\n                </ProfileCont>\n                <Link style={{ textDecoration: 'none'}} to={{ pathname: \"/EditMember\", state: {o} }}>\n                    <Edit\n                        right={current === o.id ? '0px' : '100px'}>\n                            <img src={\"edit.svg\"}/>\n                    </Edit>\n                </Link>\n                <Bin onClick={()=>{\n                    console.log(\"works\")\n                }}\n                z={current === o.id ? z 