Dep(e.target.value)\n                }}/>\n                <Input  type='text' header={'Specialization'} placeholder={'Graphic Design'} onChange={(e)=>{\n                    setSpec(e.target.value)\n                }}/>\n            </div>\n            <div className=\"saveButton\"> \n            \n            <Button path={{ pathname:'/'}} onClick={()=>{\n                    onSave(img, name, pos, dep, spec, status)\n                }}\n                width=\"100%\"\n                buttonText= \"Save Changes\"\n                />\n                \n            </div>\n        </div>\n    );\n}\n\nexport default AddMember;",{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":10,"column":3,"nodeType":"107","messageId":"108","endLine":10,"endColumn":7},{"ruleId":"105","severity":1,"message":"109","line":25,"column":11,"nodeType":"107","messageId":"108","endLine":25,"endColumn":21},{"ruleId":"105","severity":1,"message":"110","line":29,"column":11,"nodeType":"107","messageId":"108","endLine":29,"endColumn":23},{"ruleId":"111","severity":2,"message":"112","line":52,"column":53,"nodeType":"107","messageId":"113","endLine":52,"endColumn":60},{"ruleId":"105","severity":1,"message":"114","line":15,"column":16,"nodeType":"107","messageId":"108","endLine":15,"endColumn":21},{"ruleId":"105","severity":1,"message":"115","line":16,"column":17,"nodeType":"107","messageId":"108","endLine":16,"endColumn":23},{"ruleId":"105","severity":1,"message":"116","line":21,"column":20,"nodeType":"107","messageId":"108","endLine":21,"endColumn":29},{"ruleId":"117","severity":1,"message":"118","line":36,"column":21,"nodeType":"119","endLine":36,"endColumn":64},{"ruleId":"105","severity":1,"message":"120","line":3,"column":8,"nodeType":"107","messageId":"108","endLine":3,"endColumn":13},{"ruleId":"105","severity":1,"message":"121","line":54,"column":5,"nodeType":"107","messageId":"108","endLine":54,"endColumn":12},{"ruleId