-align:center;\ncolor: white;\npadding: 4px;\nposition: relative;\n`;\n\nconst Text = styled.div`\nwidth: 100%;\n`;\n\nconst IconBox = styled.div`\nmin-width: 15px;\nmin-height: 15px;\nbackground-image: url(/${props=>props.statusIcon ? props.statusIcon : \"url('/complete.svg')\"});\nbackground-size: 100% 100%;\npadding-right: 6px;\ndisplay: flex;\nalign-items: center;\njustify-content:center;\n`;\n\n\n\nconst StatusTag = ({statusText, statusIcon, bgcolor}) => {\n    // Switch statement to change colors and icon depending on the status\n    switch(statusText){\n        case \"Complete\":\n            bgcolor=\"#056571\"\n            statusIcon=\"complete.svg\"\n            break;\n\n        case \"In Progress\":\n            bgcolor=\"#32BDE9\"\n            statusIcon=\"inprogress.svg\"\n            break;\n        \n        case \"Behind\":\n            bgcolor=\"#FF7F11\"\n            statusIcon=\"behind.svg\"\n            break;\n\n        case \"Overdue\":\n            bgcolor=\"#FF3B30\"\n            statusIcon=\"overdue.svg\"\n            break;\n\n        case \"Blocked\":\n            bgcolor=\"#A1A1A1\"\n            statusIcon=\"block.svg\"\n            break;\n    }\n\n    return <Container bgcolor={bgcolor}>\n        <IconBox statusIcon={statusIcon}></IconBox><Text>{statusText}</Text>\n        </Container>\n\n}\n\nStatusTag.defaultProps = {\nbgcolor: \"#056571\",\nstatusText: \"Complete\",\nstatusIcon: \"url('/complete.svg')\"\n}\n\nexport default StatusTag;","/Users/College/Library/Mobile Documents/com~apple~CloudDocs/Documents/BCIT/WNTR 2021/Inclusio/Inclusio_DynamicContent/inclusio/src/comps/CircleButton/index.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":9,"column":3,"nodeType":"88","messageId":"89","endLine":9,"endColumn":7},{"ruleId":"90","severity":1,"message":"91","line":25,"column":7,"nodeType":"92","endLine":25,"endColumn":9,"suggestions":"93"},{"ruleId":"86","severity