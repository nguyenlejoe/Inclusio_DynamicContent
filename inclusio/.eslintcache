 useState(false)\n    const [SecondSet, setSecondSet] = useState(true)\n\n    //Slice array that is coming through if user assigns more than 4 categories\n  \n    var sliced1;\n    var sliced2;\n    \n    if(categories.length > 4){\n        sliced1 = categories.slice(0, 4)\n        sliced2 = categories.slice(4, 9)\n    }\n    console.log(sliced1, sliced2)\n\n\n    \n    return(\n        <CategoryCont>\n            {/* Mapping first and second set of categories */}\n            {sliced1 && sliced1.map(o=><CategoryTabs state={FirstSet} active={o.active} onClick={()=>{\n               if(!o.active){\n                o.active = true;  \n               }else{\n                o.active = false;\n               }\n               \n                console.log(o.active)\n            }}>{o.Department}</CategoryTabs>)}\n           \n            {sliced2 && sliced2.map(o=><CategoryTabs state={SecondSet} active={o.active}>{o.Department}</CategoryTabs>)}\n           \n           {/* Arrow that changes the state of the first set and second */}\n            <ArrowCont onClick={()=>{\n                if(!FirstSet){\n                    setFirstSet(true)\n                    setSecondSet(false)\n                }else{\n                    setFirstSet(false)\n                    setSecondSet(true)\n                }\n            }}></ArrowCont>\n        </CategoryCont>\n    );\n}\n\nCategoryBar.defaultProps = {\n    categories:fakeDpt\n}\n\nexport default CategoryBar;",{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","severity":1,"message":"51","line":123,"column":13,"nodeType":"52","endLine":123,"endColumn":38},{"ruleId":"50","severity":1,"message":"51","line":133,"column":13,"nodeType":"52","endLine":133,"endColumn":36},{"ruleId":"50","severity":1,"message":"51","line":138,"column":13,"nodeType":"52","endLine":138,"endColumn":35},{"ruleId":"46","replacedBy":"53"},{"ruleId":"48","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":1,"